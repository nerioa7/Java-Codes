	class product {
		public static void main(String[] args) {
	
	double []f_array = {21.4,3.0,4.1,5.2,6.2,43.54,23.2,54.4,65.3,54.5,65.5};
	System.out.println("This is the first array: " + f_array[2]);
		
	f_array[2] = f_array[1] + f_array[2];
	System.out.println("The answer for this sum is: " + f_array[2]);
	
		}
	
	}
